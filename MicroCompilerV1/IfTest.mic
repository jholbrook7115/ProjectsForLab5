program IfTest begin
    x := 1;
    y := 2;
    z := 3;
    if x > y then
        x := 4;
    fi;
    if x < y then
        y := 4;
    else
        z := 4;
    fi;
    if y > z then
        x := 1;
    elif z < x then
        y := 9;
    fi;
    if z = x then
        y := 99;
    elif x = y then
        z := 100;
    elif y = z then
        x := 80;
    else
        y := 1000;
    fi;
end;
